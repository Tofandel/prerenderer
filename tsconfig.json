{
  "compilerOptions": {
    "target": "es5",
    "outDir": "dist",
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    // Ensure that .d.ts files are created by tsc, but not .js files
    "declaration": true,
    "strictNullChecks": true,
    "strictBindCallApply": true,
    "lib": [
      "es6"
    ]
  }
}
