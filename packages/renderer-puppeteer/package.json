{
  "name": "@prerenderer/renderer-puppeteer",
  "version": "1.0.0",
  "description": "A renderer for @prerenderer/prerenderer that uses puppeteer to prerender pages.",
  "license": "MIT",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/cjs.js"
  },
  "types": "types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest ./test --silent",
    "test:logs": "jest ./test"
  },
  "files": [
    "dist",
    "types"
  ],
  "keywords": [
    "prerender",
    "prerenderer",
    "spa",
    "ssr",
    "serverside-rendering",
    "puppeteer",
    "google-chrome",
    "headless-chrome"
  ],
  "author": {
    "name": "Joshua Bemenderfer",
    "email": "tribex10@gmail.com",
    "url": "https://joshderf.com/"
  },
  "contributors": [
    {
      "name": "Joshua Bemenderfer",
      "email": "tribex10@gmail.com",
      "url": "https://joshderf.com/"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com/JoshTheDerf/prerenderer.git",
    "directory": "packages/renderer-puppeteer"
  },
  "bugs": {
    "url": "https://github.com/JoshTheDerf/prerenderer/issues"
  },
  "dependencies": {
    "promise-limit": "2.7.0",
    "deepmerge": "4.2.2"
  },
  "peerDependencies": {
    "puppeteer": ">= 2"
  },
  "devDependencies": {
    "puppeteer": "^19.4.1",
    "@prerenderer/prerenderer": "file:../prerenderer"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
