{
  "name": "@prerenderer/rollup-plugin",
  "version": "0.2.0",
  "description": "Flexible, framework-agnostic static site generation for apps built with rollup or vite.",
  "license": "MIT",
  "exports": {
    "import": "./index.mjs",
    "require": "./dist/cjs.js"
  },
  "types": "types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest ./test --silent",
    "test:logs": "NODE_OPTIONS=--experimental-vm-modules jest ./test"
  },
  "files": [
    "dist",
    "types",
    "index.mjs"
  ],
  "keywords": [
    "prerender",
    "ssr",
    "server-side-rendering",
    "rollup",
    "vite"
  ],
  "author": {
    "name": "Adrien Foulon",
    "email": "adrien.foulon@tukan.hu"
  },
  "contributors": [
    {
      "name": "Adrien Foulon",
      "email": "adrien.foulon@tukan.hu"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com/Tofandel/prerenderer.git",
    "directory": "packages/rollup-plugin"
  },
  "bugs": {
    "url": "https://github.com/Tofandel/prerenderer/issues"
  },
  "dependencies": {
    "@prerenderer/prerenderer": "^1.2.0",
    "schema-utils": "^4.0.0"
  },
  "optionalDependencies": {
    "@prerenderer/renderer-puppeteer": "^1.1.3"
  },
  "peerDependencies": {
    "rollup": "^3"
  },
  "devDependencies": {
    "@prerenderer/prerenderer": "file:../prerenderer",
    "@prerenderer/renderer-jsdom": "file:../renderer-jsdom",
    "@prerenderer/renderer-puppeteer": "file:../renderer-puppeteer",
    "@vitejs/plugin-vue": "^4.1.0",
    "express": "^4.18.2",
    "rollup": "^3.20.0",
    "vite": "^4.2.1",
    "vue": "^3.2.47",
    "vue-loader": "^17.0.1",
    "vue-router": "^4.1.6"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "jest": {
    "testTimeout": 10000,
    "testMatch": [
      "**/__tests__/**/*.?(m)[jt]s?(x)",
      "**/?(*.)+(spec|test).?(m)[tj]s?(x)"
    ]
  },
  "gitHead": "197a8979e6b8b3a8f2d990e92d01b4d82e422f07"
}
