{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "test:logs"
        ]
      }
    }
  },
  "namedInputs": {
    "sources": [
      "{projectRoot}/tsconfig.json",
      "{projectRoot}/package-lock.json",
      "{projectRoot}/packages/prerenderer/src/*",
      "{workspaceRoot}/src/*.ts",
      "{workspaceRoot}/tsconfig.json",
      "{workspaceRoot}/package.json"
    ]
  },
  "targetDefaults": {
    "lint:fix": {
      "inputs": [
        "sources"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {
      "inputs": [
        "sources"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "build": {
      "inputs": [
        "sources"
      ],
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/types"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ]
    },
    "prepublishOnly": {
      "dependsOn": [
        "^prepublishOnly"
      ]
    }
  }
}
