{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "test:logs"
        ]
      }
    }
  },
  "targetDefaults": {
    "test": {
      "dependsOn": [
        "^build"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/types"
      ]
    },
    "prepublishOnly": {
      "dependsOn": [
        "^prepublishOnly"
      ]
    }
  }
}
